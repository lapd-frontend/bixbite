{
    "name": "russsiq/bixbite",
    "description": "Система для оперативного развертывания простейших сайтов на shared хостингах на базе Laravel.",
    "keywords": [
        "bixbite",
        "cms",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "version": "dev-master",
    "authors": [
        {
            "name": "russsiq",
            "email": "rusiq@list.ru"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.1",
        "laravel/tinker": "^1.0",
        "russsiq/bixbite": "dev-master",
        "russsiq/laravel-assistant": "dev-master",
        "russsiq/laravel-env-manager": "dev-master",
        "russsiq/laravel-grecaptcha": "dev-master"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/russsiq/bixbite"
        }
    ],
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "BBCMS\\": "app/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --no-ansi"
        ],
        "post-update-cmd": [
          "Illuminate\\Foundation\\ComposerScripts::postUpdate",
          "php artisan optimize"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
